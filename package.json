    "cross-env": "^7.0.3",
    "fs-extra": "^11.1.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "sass": "^1.58.3",
    "sitemap": "^7.1.1",
    "vitepress": "1.0.0-alpha.72",
    "vue": "^3.2.47"
  },
  "lint-staged": {
    "*.{js,ts,css,md,json}": [
      "prettier --write"
    ]
